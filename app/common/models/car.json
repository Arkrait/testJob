{
  "name": "Car",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "plate": {
      "type": "string",
      "required": true
    },
    "brand": {
      "type": "string",
      "required": true
    },
    "model": {
      "type": "string",
      "required": true
    },
    "ownerCredentials": {
      "type": "string",
      "required": true
    },
    "horsePower": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {
    "requestRegister": {
      "accepts": [
        { "arg": "id", "type": "string" },
        { "arg": "privateKey", "type": "string" },
        { "arg": "options", "type": "object" }
      ],
      "returns": { "arg": "greeting", "type": "string" },
      "http": { "verb": "post", "path": "/:id/requestRegister/:privateKey" }
    },
    "test": {
      "accepts": { "arg": "anything", "type": "string" },
      "returns": { "arg": "return", "type": "string" },
      "http": {
        "verb": "get",
        "path": "/test"
      }
    }
  }
}
